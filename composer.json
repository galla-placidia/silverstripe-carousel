{
    "name": "galla-placidia/silverstripe-carousel",
    "description": "A new page type that provides a carousel with easily reorderable items and custom HTML captions",
    "type": "silverstripe-module",
    "keywords": ["silverstripe", "carousel", "bootstrap", "bxslider", "silverstrap"],
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Marlis Heck",
            "email": "info@gama-web.de",
            "homepage": "https://www.gama-web.de/"            
        }
    ],
    "require": {
        "silverstripe/cms": "^4.1",
        "bummzack/sortablefile": "^2"
    },
    "extra": {
        "installer-name": "carousel",
        "branch-alias": {
            "dev-master": "2.2.x-dev",
            "dev-dev": "2.2.x-dev"
        },
        "screenshots": [
            "http://silverstripe.entidi.com/assets/carousel.jpeg"
        ]
    },
    "suggest": {
        "entidi/silverstrap": "A silverstripe theme that supports silverstripe-carousel out of the box"
    }
}
